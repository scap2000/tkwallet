<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="2021051501" id="org.tkcoin.wallet" ios-CFBundleVersion="7" version="1.0.4" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>TKCoin</name>
    <description>
      Secure EDUCoin/TKCoin Wallet
    </description>
    <author email="support@tkcoin.org" href="https://tkcoin.org">
        TKCoin Org.
    </author>
    <content src="index.html" />
    <access origin="https://bws.tkcoin.org" />
    <access origin="https://tkcoin.org" />
    <access origin="https://test.tkcoin.org" />
    <access origin="https://staging.tkcoin.org" />
    <access origin="https://auth.shapeshift.io" />
    <access origin="https://shapeshift.io" />
    <access origin="https://api.coinbase.com" />
    <access origin="https://coinbase.com" />
    <access origin="https://www.coinbase.com" />
    <access origin="https://api.testwyre.com" />
    <access origin="https://api.sendwyre.com" />
    <allow-navigation href="gap://ready" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardResize" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="false" />
    <preference name="KeyboardResizeMode" value="ionic" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="Orientation" value="default" />
    <preference name="cordova-custom-config-stoponerror" value="true" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ffffff" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value="200" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="LoadUrlTimeoutValue" value="60000" />
    <preference name="UseSwiftLanguageVersion" value="5" />
    <preference name="android-minSdkVersion" value="23" />
    <preference name="android-targetSdkVersion" value="29" />
    <preference name="deployment-target" value="10.3" />
    <engine name="android" spec="9.0.0" />
    <engine name="ios" spec="5.1.1" />
    <plugin name="cordova-plugin-advanced-http" spec="2.0.8" />
    <plugin name="cordova-launch-review" spec="3.1.1" />
    <plugin name="cordova-clipboard" spec="1.1.1" />
    <plugin name="cordova-plugin-device" spec="2.0.3" />
    <plugin name="cordova-plugin-globalization" spec="1.0.9" />
    <plugin name="cordova-plugin-qrscanner" spec="3.0.1" />
    <plugin name="cordova-plugin-add-swift-support" spec="2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="3.2.0" />
    <plugin name="cordova-plugin-fingerprint-aio" spec="3.0.1">
        <variable name="FACEID_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="1.3.4" />
    <plugin name="cordova-plugin-x-socialsharing" spec="5.6.8" />
    <plugin name="cordova-plugin-x-toast" spec="2.6.0" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="2.1.3" />
    <plugin name="cordova-plugin-screen-orientation" spec="3.0.2" />
    <plugin name="im.ltdev.cordova.UserAgent" spec="https://github.com/danielsogl/cordova-plugin-useragent.git" />
    <plugin name="cordova-apple-wallet-ng" spec="1.3.1" />
    <plugin name="cordova-plugin-spinner-dialog" spec="1.3.1" />
    <plugin name="cordova-plugin-dialogs" spec="2.0.2" />
    <plugin name="cordova-plugin-vibration" spec="3.1.1" />
    <plugin name="cordova-plugin-file" spec="6.0.2" />
    <plugin name="cordova-custom-config" spec="5.1.0" />
    <plugin name="cordova-plugin-customurlscheme-ng" spec="10.0.0">
        <variable name="URL_SCHEME" value="bitcoin" />
        <variable name="SECOND_URL_SCHEME" value="tkcoin" />
        <variable name="THIRD_URL_SCHEME" value="bitcoincash" />
        <variable name="FOURTH_URL_SCHEME" value="bchtest" />
        <variable name="FIFTH_URL_SCHEME" value="ethereum" />
        <variable name="SIXTH_URL_SCHEME" value="ripple" />
        <variable name="SEVENTH_URL_SCHEME" value="wc" />
        <variable name="EIGHTH_URL_SCHEME" value="dogecoin" />
        <variable name="ANDROID_SCHEME" value="" />
        <variable name="ANDROID_HOST" value="" />
        <variable name="ANDROID_PATHPREFIX" value="" />
    </plugin>
    <plugin name="cordova-plugin-fcm-ng" spec="8.3.0">
        <variable name="PAGE_LINK_DOMAIN" value="tkcoinapp.page.link" />
    </plugin>
    <plugin name="cordova-plugin-wkwebview-engine" spec="1.2.1" />
    <plugin name="cordova-plugin-theme-detection" spec="1.2.1" />
    <platform name="ios">
        <config-file parent="UIStatusBarHidden" platform="ios" target="*-Info.plist">
            <true />
        </config-file>
        <config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios" target="*-Info.plist">
            <true />
        </config-file>
        <config-file parent="ITSAppUsesNonExemptEncryption" target="*-Info.plist">
            <false />
        </config-file>
        <config-file parent="LSApplicationQueriesSchemes" platform="ios" target="*-Info.plist">
            <array>
                <string>whatsapp</string>
                <string>fb</string>
                <string>twitter</string>
            </array>
        </config-file>
        <config-file mode="add" parent="com.apple.developer.pass-type-identifiers" target="Entitlements-Release.plist">
            <array>
                <string>$(TeamIdentifierPrefix)*</string>
            </array>
        </config-file>
        <config-file mode="add" parent="com.apple.developer.associated-domains" target="Entitlements-Release.plist">
            <array>
                <string>applinks:tkcoinapp.page.link</string>
            </array>
        </config-file>
        <icon height="180" src="resources/tkcoin/ios/icon/icon-60@3x.png" width="180" />
        <icon height="60" src="resources/tkcoin/ios/icon/icon-60.png" width="60" />
        <icon height="120" src="resources/tkcoin/ios/icon/icon-60@2x.png" width="120" />
