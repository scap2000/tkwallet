        <icon background="resources/tkcoin/android/icon/new/mipmap-xxhdpi/ic_launcher_background.png" density="xxhdpi" foreground="resources/tkcoin/android/icon/new/mipmap-xxhdpi/ic_launcher_foreground.png" />
