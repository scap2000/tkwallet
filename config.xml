        <icon height="57" src="resources/tkcoin/ios/icon/icon.png" width="57" />
        <icon height="114" src="resources/tkcoin/ios/icon/icon@2x.png" width="114" />
        <icon height="72" src="resources/tkcoin/ios/icon/icon-72.png" width="72" />
        <icon height="144" src="resources/tkcoin/ios/icon/icon-72@2x.png" width="144" />
        <icon height="29" src="resources/tkcoin/ios/icon/icon-small.png" width="29" />
        <icon height="58" src="resources/tkcoin/ios/icon/icon-small@2x.png" width="58" />
        <icon height="87" src="resources/tkcoin/ios/icon/icon-small@3x.png" width="87" />
        <icon height="50" src="resources/tkcoin/ios/icon/icon-50.png" width="50" />
        <icon height="100" src="resources/tkcoin/ios/icon/icon-50@2x.png" width="100" />
        <icon height="1024" src="resources/tkcoin/ios/app-store/icon-1024.png" width="1024" />
        <splash src="resources/tkcoin/ios/splash/Default@2x~universal~anyany.png" />
        <splash src="resources/tkcoin/ios/splash/Default@3x~universal~anyany.png" />
    </platform>
    <platform name="android">
        <preference name="android-manifest/application/@android:allowBackup" value="false" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:icon="@mipmap/ic_launcher" />
        </edit-config>
        <icon background="resources/tkcoin/android/icon/new/mipmap-mdpi/ic_launcher_background.png" density="mdpi" foreground="resources/tkcoin/android/icon/new/mipmap-mdpi/ic_launcher_foreground.png" />
        <icon background="resources/tkcoin/android/icon/new/mipmap-hdpi/ic_launcher_background.png" density="hdpi" foreground="resources/tkcoin/android/icon/new/mipmap-hdpi/ic_launcher_foreground.png" />
